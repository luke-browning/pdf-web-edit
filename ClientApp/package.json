    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --host 0.0.0.0 --port 44437 --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
